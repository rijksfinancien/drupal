<div id="search-container" class="container">
  <div class="facets">
    <h2>{{ 'Filter your results' |t }}</h2>
    {{ facets }}
  </div>
  <div class="search-results">
    <h1 class="page-title">{{ title }}</h1>
    <div class="search-top-bar">
      {{ pagination }}
      {{ sorting }}
    </div>
    <div class="result--list">
      {% if rows %}
        <div>
          {% for row in rows %}
            {{ row }}
          {% endfor %}
        </div>
      {% endif %}
    </div>
    {{ pagination }}
  </div>
</div>
